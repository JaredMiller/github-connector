        <github:add-emails>
            <github:emails>
                <github:email>email1</github:email>
                <github:email>email2</github:email>
            </github:emails>
        </github:add-emails>
    </flow>

    <flow name="removeEmails">
        <github:remove-emails>
            <github:emails>
                <github:email>email1</github:email>
                <github:email>email2</github:email>
            </github:emails>
        </github:remove-emails>
    </flow>

    <flow name="getKeys">
        <github:get-keys/>
    </flow>

    <flow name="getKey">
        <github:get-key id="1"/>
    </flow>

    <flow name="createKey">
        <github:create-key title="title" key="key"/>
